{
  "name": "@kevinmind/booti",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build --compile ./src/index.ts --outfile ../dist/booti",
    "build:darwin-arm64": "bun build --compile --target=bun-darwin-arm64 ./src/index.ts --outfile ../dist/booti-darwin-arm64",
    "build:darwin-x64": "bun build --compile --target=bun-darwin-x64 ./src/index.ts --outfile ../dist/booti-darwin-x64",
    "build:all": "bun run build:darwin-arm64 && bun run build:darwin-x64",
    "test": "bun test"
  },
  "dependencies": {
    "zod": "^3.23.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.4.0"
  }
}
